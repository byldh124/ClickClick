<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Button">
        <item name="android:fontFamily">@font/nanum_square_round_eb</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:elevation">2dp</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingBottom">12dp</item>
        <item name="android:paddingStart">32dp</item>
        <item name="android:paddingEnd">32dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="ButtonBlue01" parent="Button">
        <item name="android:background">@drawable/button_blue01</item>
    </style>

    <style name="ButtonOrange01" parent="Button">
        <item name="android:background">@drawable/button_orange01</item>
    </style>
</resources>